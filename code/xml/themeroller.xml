		<white_lines>50%,50%,repeat</white_lines>
		<gloss_wave>50%,50%,repeat-x</gloss_wave>
		<diamond>50%,50%,repeat</diamond>
		<loop>50%,50%,repeat</loop>
		<carbon_fiber>50%,50%,repeat</carbon_fiber>
