	<!-- XPOS, YPOS AND REPEAT TEXTURES CSS -->
	<csstrick>
		<flat>50%,50%,repeat-x</flat>
		<glass>50%,50%,repeat-x</glass>
		<highlight_soft>50%,top,repeat-x</highlight_soft>
		<highlight_hard>50%,top,repeat-x</highlight_hard>
		<inset_soft>50%,bottom,repeat-x</inset_soft>
		<inset_hard>50%,bottom,repeat-x</inset_hard>
		<diagonals_small>50%,50%,repeat-xy</diagonals_small>
		<diagonals_medium>50%,50%,repeat-xy</diagonals_medium>
		<diagonals_thick>50%,50%,repeat-xy</diagonals_thick>
		<dots_small>50%,50%,repeat-xy</dots_small>
		<dots_medium>50%,50%,repeat-xy</dots_medium>
		<white_lines>50%,50%,repeat-xy</white_lines>
		<gloss_wave>50%,50%,repeat-x</gloss_wave>
		<diamond>50%,50%,repeat-xy</diamond>
		<loop>50%,50%,repeat-xy</loop>
		<carbon_fiber>50%,50%,repeat-xy</carbon_fiber>
		<diagonal_maze>50%,50%,repeat-xy</diagonal_maze>
		<diamond_ripple>50%,50%,repeat-xy</diamond_ripple>
		<hexagon>50%,50%,repeat-xy</hexagon>
		<layered_circles>50%,50%,repeat-xy</layered_circles>
		<boxes>50%,50%,repeat-xy</boxes>
		<glow_ball>50%,top,no-repeat</glow_ball>
		<spotlight>50%,top,no-repeat</spotlight>
		<fine_grain>50%,50%,repeat-xy</fine_grain>
	</csstrick>
	<!-- RGB COLOR PERMUTATIONS -->
	<themes>
		<blue>rgb</blue>
		<blue2>rrg</blue2>
		<blue3>rrb</blue3>
		<red>brr</red>
		<pink>brb</pink>
		<brown>bgr</brown>
		<brown2>grr</brown2>
		<violet>grg</violet>
		<violet2>grb</violet2>
		<green>rgr</green>
		<green2>rgg</green2>
		<black>rrr</black>
	</themes>
</root>
