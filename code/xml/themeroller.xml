	<!-- XPOS, YPOS AND REPEAT TEXTURES CSS -->
	<csstrick>
		<flat>50%,50%,repeat-x</flat>
		<glass>50%,50%,repeat-x</glass>
		<highlight_soft>50%,top,repeat-x</highlight_soft>
		<highlight_hard>50%,top,repeat-x</highlight_hard>
		<inset_soft>50%,bottom,repeat-x</inset_soft>
		<inset_hard>50%,bottom,repeat-x</inset_hard>
		<diagonals_small>50%,50%,repeat</diagonals_small>
		<diagonals_medium>50%,50%,repeat</diagonals_medium>
