<root>
	<name>style</name>
	<label lang="true">style</label>
	<type>select</type>
	<width>120px</width>
	<rows>
		<row>
			<label>Blue (default)</label>
			<value>blue</value>
		</row>
		<row>
			<label>Pink</label>
			<value>pink</value>
		</row>
		<row>
			<label>Red</label>
			<value>red</value>
		</row>
		<row>
			<label>Orange</label>
			<value>orange</value>
		</row>
		<row>
			<label>Yellow</label>
			<value>yellow</value>
		</row>
		<row>
			<label>Brown</label>
			<value>brown</value>
		</row>
		<row>
			<label>Green</label>
			<value>green</value>
		</row>
		<row>
			<label>Cyan</label>
			<value>cyan</value>
		</row>
		<row>
			<label>Gray</label>
			<value>gray</value>
		</row>
		<row>
			<label>Blue (inverted)</label>
			<value>invblue</value>
		</row>
		<row>
			<label>Pink (inverted)</label>
			<value>invpink</value>
		</row>
		<row>
			<label>Red (inverted)</label>
			<value>invred</value>
		</row>
		<row>
			<label>Orange (inverted)</label>
			<value>invorange</value>
		</row>
		<row>
			<label>Yellow (inverted)</label>
			<value>invyellow</value>
		</row>
		<row>
			<label>Brown (inverted)</label>
			<value>invbrown</value>
		</row>
		<row>
			<label>Green (inverted)</label>
			<value>invgreen</value>
		</row>
		<row>
			<label>Cyan (inverted)</label>
			<value>invcyan</value>
		</row>
		<row>
			<label>Gray (inverted)</label>
			<value>invgray</value>
		</row>
	</rows>
	<value global="style" eval="true">$style</value>
	<onchange>submit1()</onchange>
</root>
